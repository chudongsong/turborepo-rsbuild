{
  "name": "linglongos",
  "private": true,
  "scripts": {
    "with-env": "dotenv -e ./.env --",
    "build": "pnpm with-env turbo build",
    "dev": "pnpm with-env turbo dev",
    "lint": "turbo lint",
    "test": "turbo run test",
    "type-check": "turbo run type-check",
    "format": "biome format --write .",
    "check": "biome check --write .",
    "clean": "bash scripts/clean-modules.sh",
    "biome:check": "biome check .",
    "biome:check:fix": "biome check --write .",
    "biome:format": "biome format .",
    "biome:format:fix": "biome format --write .",
    "biome:lint": "biome lint .",
    "biome:lint:fix": "biome lint --write .",
    "biome:ci": "biome ci .",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.3",
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@types/node": "^24.10.0",
    "dotenv-cli": "^11.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "turbo": "^2.6.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@rematch/core": "^2.2.0",
    "react-redux": "^9.2.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "pnpm": "^9.9.0",
    "typescript": "^5.7.3",
    "vite": "^5.3.3"
  },
  "packageManager": "pnpm@10.3.0",
  "engines": {
    "node": ">=18"
  },
  "pnpm": {
    "overrides": {
      "react": "19.2.0",
      "react-dom": "19.2.0",
      "@types/react": "^18.3.26",
      "@types/react-dom": "^18.3.7"
    },
    "onlyBuiltDependencies": [
      "@eggjs/bin",
      "better-sqlite3",
      "core-js",
      "cpu-features",
      "esbuild",
      "sqlite3",
      "ssh2",
      "vue-demi"
    ]
  }
