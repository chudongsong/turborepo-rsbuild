{
  "name": "@linglongos/hooks",
  "version": "1.0.0",
  "description": "通用 React Hooks 库",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "tsc -w",
    "lint": "biome check ./src",
    "lint:fix": "biome check --write ./src",
    "format": "biome format ./src",
    "format:fix": "biome format --write ./src",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "axios": "^1.7.2",
    "md5": "^2.3.0",
    "ramda": "^0.30.1"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": false
    },
    "react-dom": {
      "optional": false
    }
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.2",
    "@types/md5": "^2.3.5",
    "@types/ramda": "^0.29.12",
    "@types/react": "^18.3.26",
    "@types/react-dom": "^18.3.7",
    "@vitest/coverage-v8": "^3.2.4",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "keywords": [
    "react",
    "hooks",
    "typescript",
    "utilities"
  ],
  "author": "",
  "license": "MIT"
}
